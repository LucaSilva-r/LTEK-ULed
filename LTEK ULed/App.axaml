<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="LTEK_ULed.App"
             xmlns:semi="https://irihi.tech/semi"
			 RequestedThemeVariant="Default">

	<Application.Styles>
		<semi:SemiTheme/>
	</Application.Styles>
	
	<Application.Resources>
		<FontFamily x:Key="BootstrapIcons">avares://LTEK ULed/Assets#bootstrap-icons</FontFamily>
	</Application.Resources>

	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.ThemeDictionaries>
				<ResourceDictionary x:Key='Light'>
					<SolidColorBrush x:Key='BackgroundBrush'>LightGray</SolidColorBrush>
				</ResourceDictionary>
				<ResourceDictionary x:Key='Dark'>
					<SolidColorBrush x:Key='BackgroundBrush'>White</SolidColorBrush>
				</ResourceDictionary>
			</ResourceDictionary.ThemeDictionaries>
		</ResourceDictionary>
	</Application.Resources>

</Application>
